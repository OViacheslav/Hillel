def which_is_greater(a, b):
    if a > b:
        return a
    return b


def which_is_less(a, b, c):
    if c > a < b:
        return a
    if b < c:
        return b
    return c


def absolute_value_of_a_number(a):
    return abs(a)


def sum_of_entering_value(a, b):
    print(a + b)


def is_positive(a):
    if a > 0:
        print('Positive')
    elif a == 0:
        print('Neither nor')
    else:
        print('Negative')
